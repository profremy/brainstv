    "hpp": "^0.2.3",
    "jquery": "^3.5.1",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "mongoose": "^5.10.17",
    "multer": "^1.4.2",
    "nodemailer": "^6.5.0",
    "slugify": "^1.5.0",
    "validator": "^13.5.2",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.6",
    "parcel-bundler": "^1.12.5"
  }
