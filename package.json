{
  "name": "brainstv",
  "version": "1.0.0",
  "description": "Children's TV Show",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "prodStart": "cross-env  NODE_ENV=production nodemon server.js",
    "devStart": "cross-env NODE_ENV=development nodemon server.js",
    "watch:js": "parcel watch ./public/javascript/script.js --out-dir ./public/javascript --out-file bundle.js",
