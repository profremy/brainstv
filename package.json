  "license": "ISC",
  "engines": {
    "node": ">=12.16.3"
  },
  "bugs": {
    "url": "https://github.com/profremy/brainstv/issues"
  },
  "homepage": "https://github.com/profremy/brainstv#readme",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-mongo-sanitize": "^2.0.2",
    "express-rate-limit": "^5.2.6",
