  <h3 class="text-center text-dark">Birthday Celebrants</h3>

  <div class="btvbirthday-slider">
    <div class="btvbirthday-slider__wrp swiper-wrapper">
      <% birthdayCelebrants.forEach(celebrant => { %>
      <div class="btvbirthday-slider__item swiper-slide">
        <div class="btvbirthday-slider__img">

          <img src="<%= celebrant.birthdayCardPath %>" alt="Birthday celebrant">
        </div>
        <div class="btvbirthday-slider__content">
          <!-- <span class="btvbirthday-slider__code">26 December 2019</span> -->
          <div class="btvbirthday-slider__title"><%= celebrant.fullName %></div>
          <div class="btvbirthday-slider__text">"<%= celebrant.birthdayMessage %>"</div>
          <div class="btvbirthday-slider__text text-dark font-italic "><small>Do you want your birthday card here? Join
              the
              Brains
              club.</small></div>
          <a href="/clubmembers/joinbrainsclub" class="btn btn-primary _btvbirthday-slider__button">Click
            Here</a>
        </div>
      </div>

      <% }) %>
    </div>
    <div class="btvbirthday-slider__pagination"></div>
  </div>

  <div class="section__recent__shows mt-3 p-3 timber-gradient">
    <div class="auto-grid-front-page-shows">
      <% eightRecentShows.forEach(recentShow => { %>
      <div class="autoGridPhotos transform-scale">
        <a href="/brainstv/show/<%= recentShow.slug %>">
          <div class="noBorderResourcesOverlay"></div>
        </a>
        <!-- <div class="learningResourcesTopTitle"><%#= recentShow.showStatus %> </div> -->

        <img src="/images/show_images/<%= recentShow.showThumbnail %>" width="100%" height="100%" class="_img-fluid"
          alt="<%= recentShow.showTitle %>">

        <div class="noBorderLearningResourcesBottomTitle"><%= recentShow.showTitle %> </div>
      </div>
      <% }) %>
    </div>
    <div class="text-center">
      <a href="/brainstv/shows" class="btn btn-outline-light mt-3">All Shows</a>
    </div>
  </div>
</div>

