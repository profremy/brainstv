<div class="jumbotron container-fluid timber-gradient pb-1">
    <div class="input-group margin-bottom-sm">
        <span class="input-group-addon bg-light p-1"><i class="fa fa-play shows" aria-hidden="true"></i></span>
        <span class="dark-blue-gradient-background p-1">All Videos</span>
    </div>

    <div class="row">
        <div class="col-sm-9">
            <div class="tab-content" id="v-pills-tabContent">
                <div class="tab-pane fade show active" id="v-pills-default" role="tabpanel"
                    aria-labelledby="v-pills-default-tab">
                    <div class="p-2 w-100 bd-highlight mobileScreen mb-3">
                        <div class="embed-responsive embed-responsive-16by9">
                            <iframe class="responsive-iframe" width="560" height="315"
                                src="https://www.youtube-nocookie.com/embed/<%= newsUpdateVideos[0].showId %>?rel=0&modestbranding=1"
                                frameborder="0"
                                allow="accelerometer; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                                allowfullscreen></iframe>
                        </div>
                    </div>

                    <div class="p-2 w-100 bd-highlight desktopScreen mr-2 mb-3">
                        <div class="embed-responsive embed-responsive-16by9">
                            <iframe class="responsive-iframe" width="560" height="315"
                                src="https://www.youtube-nocookie.com/embed/<%= newsUpdateVideos[0].showId %>?rel=0&modestbranding=1"
                                frameborder="0"
                                allow="accelerometer; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                                allowfullscreen></iframe>
                        </div>
                    </div>
                </div>
                <div class="tab-pane fade" id="v-pills-next-1" role="tabpanel" aria-labelledby="v-pills-next-1-tab">
                    <div class="tab-pane fade show active" id="v-pills-default" role="tabpanel"
                        aria-labelledby="v-pills-default-tab">
                        <div class="p-2 w-100 bd-highlight mobileScreen mb-3">
                            <div class="embed-responsive embed-responsive-16by9">
                                <iframe class="responsive-iframe" width="560" height="315"
                                    src="https://www.youtube-nocookie.com/embed/<%= newsUpdateVideos[1].showId %>?rel=0&modestbranding=1"
                                    frameborder="0"
                                    allow="accelerometer; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                                    allowfullscreen></iframe>
                            </div>
                        </div>

                        <div class="p-2 w-100 bd-highlight desktopScreen mr-2 mb-3">
                            <div class="embed-responsive embed-responsive-16by9">
                                <iframe class="responsive-iframe" width="560" height="315"
                                    src="https://www.youtube-nocookie.com/embed/<%= newsUpdateVideos[1].showId %>?rel=0&modestbranding=1"
                                    frameborder="0"
                                    allow="accelerometer; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                                    allowfullscreen></iframe>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="tab-pane fade" id="v-pills-next-2" role="tabpanel" aria-labelledby="v-pills-next-2-tab">
                    <div class="tab-pane fade show active" id="v-pills-default" role="tabpanel"
                        aria-labelledby="v-pills-default-tab">
                        <div class="p-2 w-100 bd-highlight mobileScreen mb-3">
                            <div class="embed-responsive embed-responsive-16by9">
                                <iframe class="responsive-iframe" width="560" height="315"
                                    src="https://www.youtube-nocookie.com/embed/<%= newsUpdateVideos[2].showId %>?rel=0&modestbranding=1"
                                    frameborder="0"
                                    allow="accelerometer; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                                    allowfullscreen></iframe>
                            </div>
                        </div>

                        <div class="p-2 w-100 bd-highlight desktopScreen mr-2 mb-3">
                            <div class="embed-responsive embed-responsive-16by9">
                                <iframe class="responsive-iframe" width="560" height="315"
                                    src="https://www.youtube-nocookie.com/embed/<%= newsUpdateVideos[2].showId %>?rel=0&modestbranding=1"
                                    frameborder="0"
                                    allow="accelerometer; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                                    allowfullscreen></iframe>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-3 d-flex flex-column justify-content-around wideTablets">
            <div class="input-group margin-bottom-sm">
                <span class="dark-blue-gradient-background py-1 px-3">Next Video</span>
            </div>
            <div class="nav flex-column _nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical">
                <a class="nav-link active hideElement" id="v-pills-default-tab" data-toggle="pill"
                    href="#v-pills-default" role="tab" aria-controls="v-pills-default" aria-selected="true">Latest
                    Video</a>
                <a class="nav-link" id="v-pills-next-1-tab" data-toggle="pill" href="#v-pills-next-1" role="tab"
                    aria-controls="v-pills-next-1" aria-selected="false">
                    <div class="sideThumbnails transform-scale">
                        <img src="/images/show_images/<%= newsUpdateVideos[1].showThumbnail %>" width="100%"
                            height="180px" class="_myImg-fluid _img-fluid" alt="<%= newsUpdateVideos[1].showTitle %>">
                        <div class="noBorderLearningResourcesBottomTitle"><%= newsUpdateVideos[1].showTitle %>
                        </div>
                    </div>
                </a>
                <a class="nav-link" id="v-pills-next-2-tab" data-toggle="pill" href="#v-pills-next-2" role="tab"
                    aria-controls="v-pills-next-2" aria-selected="false">
                    <div class="sideThumbnails transform-scale">
                        <img src="/images/show_images/<%= newsUpdateVideos[2].showThumbnail %>" width="100%"
                            height="180px" class="_myImg-fluid _img-fluid" alt="<%= newsUpdateVideos[2].showTitle %>">
                        <div class="noBorderLearningResourcesBottomTitle"><%= newsUpdateVideos[2].showTitle %>
                        </div>
                    </div>
                </a>
            </div>
        </div>
    </div>


    <ul class="nav nav-pills mb-3 mt-3" id="pills-tab" role="tablist">
        <li class="nav-item mr-auto" role="presentation">
            <a class="nav-link btn btn-outline-light rounded-pill active" id="pills-sortA-Z-tab" data-toggle="pill"
                href="#pills-sortA-Z" role="tab" aria-controls="pills-sortA-Z" aria-selected="true">Sort Alphabetically
                (A-Z)</a>
        </li>
        <li class="nav-item mr-auto" role="presentation">
            <a class="nav-link btn btn-outline-light rounded-pill" id="pills-sortByDate-tab" data-toggle="pill"
                href="#pills-sortByDate" role="tab" aria-controls="pills-sortByDate" aria-selected="false">Sort By
                Date</a>
        </li>
    </ul>
    <div class="tab-content" id="pills-tabContent">
        <div class="tab-pane fade show active" id="pills-sortA-Z" role="tabpanel" aria-labelledby="pills-sortA-Z-tab">

            <div class="section__recent__videos mt-3 mb-3 p-3" style="background-color: #7eb379;">
                <div class="auto-grid-front-page-shows">
                    <% allShowsA_Z.forEach(show => { %>
                    <% if(show.showType === "Video") { %>
                    <div class="autoGridPhotos transform-scale">
                        <a href="/brainstv/show/<%= show.slug %>">
                            <div class="noBorderResourcesOverlay"></div>
                        </a>
                        <div class="learningResourcesTopTitle"><%#= show.showStatus %> </div>

                        <img src="/images/show_images/<%= show.showThumbnail %>" width="100%" height="200"
                            class="img-fluid" alt="<%= show.showTitle %>">

                        <div class="noBorderLearningResourcesBottomTitle"><%= show.showTitle %> </div>
                    </div>
                    <% } %>
                    <% }) %>
                </div>
                <div class="text-center">
                    <a href="/brainstv/videos" class="btn btn-outline-light mt-3">All Videos</a>
                </div>
            </div>






