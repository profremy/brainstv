<div class="jumbotron container-fluid">
    <div class="input-group margin-bottom-sm">
        <span class="input-group-addon bg-light p-1"><i class="fa fa-cubes shows" aria-hidden="true"></i></span>
        <span class="dark-blue-gradient-background p-1">All Activities</span>
    </div>

    <hr class="my-1 document-divider">
    <ul class="nav nav-pills mb-3 mt-3" id="pills-tab" role="tablist">
        <li class="nav-item mr-auto" role="presentation">
            <a class="nav-link btn btn-outline-info rounded-pill active" id="pills-sortA-Z-tab" data-toggle="pill"
                href="#pills-sortA-Z" role="tab" aria-controls="pills-sortA-Z" aria-selected="true">Sort Alphabetically
                (A-Z)</a>
        </li>
        <li class="nav-item mr-auto" role="presentation">
            <a class="nav-link btn btn-outline-info rounded-pill" id="pills-sortByDate-tab" data-toggle="pill"
                href="#pills-sortByDate" role="tab" aria-controls="pills-sortByDate" aria-selected="false">Sort By
                Date</a>
        </li>
    </ul>
    <div class="tab-content" id="pills-tabContent">
        <div class="tab-pane fade show active" id="pills-sortA-Z" role="tabpanel" aria-labelledby="pills-sortA-Z-tab">
            <div class="main-flex">

                <% allShowsA_Z.forEach(show => { %>
                <% if(show.showType === "Activity") { %>
                <div class="col-sm-3 flex-item">
                    <div class="embed-responsive _embed-responsive-16by9 transform-scale">
                        <a href="/brainstv/show/<%= show.slug %>">
                            <div class="resourcesOverlay"></div>
                        </a>
                        <div class="learningResourcesTopTitle"><%= show.showStatus %> </div>
                        <!-- <iframe width="560" height="315"
                            src="https://www.youtube-nocookie.com/embed/<%#= show.showId %>?rel=0&modestbranding=1"
                            frameborder="0"
                            allow="accelerometer; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                            allowfullscreen></iframe> -->

                        <img src="/images/show_images/<%= show.showThumbnail %>" width="315" height="200"
                            class="_img-fluid" alt="<%= show.showTitle %>">
                        <div class="learningResourcesBottomTitle"><%= show.showTitle %></div>
                        <!-- <%# if (show.className.className.startsWith('Y')) { %>
                        <div class="learningResourcesBottomTitle"><%#= show.className.className.split('E')[0]%> </div>
                        <%# } else { %>
                        <div class="learningResourcesBottomTitle"><%#= show.className.className %> </div>
                        <%# } %> -->
                    </div>
                </div>
                <% } %>
                <% }) %>

            </div>
        </div>
        <div class="tab-pane fade" id="pills-sortByDate" role="tabpanel" aria-labelledby="pills-sortByDate-tab">
            <div class="main-flex">

                <% allShows.forEach(show => { %>
                <% if(show.showType === "Activity") { %>
                <div class="col-sm-3 flex-item">
                    <div class="embed-responsive _embed-responsive-16by9 transform-scale">
                        <a href="/brainstv/show/<%= show.slug %>">
                            <div class="resourcesOverlay"></div>
                        </a>
                        <div class="learningResourcesTopTitle"><%= show.showStatus %> </div>
                        <!-- <iframe width="560" height="315"
                            src="https://www.youtube-nocookie.com/embed/<%#= show.showId %>?rel=0&modestbranding=1"
                            frameborder="0"
                            allow="accelerometer; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
                            allowfullscreen></iframe> -->

                        <img src="/images/show_images/<%= show.showThumbnail %>" width="315" height="200"
                            class="_img-fluid" alt="<%= show.showTitle %>">
                        <div class="learningResourcesBottomTitle"><%= show.showTitle %></div>
                        <!-- <%# if (show.className.className.startsWith('Y')) { %>
                        <div class="learningResourcesBottomTitle"><%#= show.className.className.split('E')[0]%> </div>
                        <%# } else { %>
                        <div class="learningResourcesBottomTitle"><%#= show.className.className %> </div>
                        <%# } %> -->
                    </div>
                </div>
                <% } %>
                <% }) %>

            </div>
        </div>
    </div>


    <div class="section__recent__takePart _mt-3 p-3" style="background-color: #355394;">
        <h3 class="text-center text-light">Trending Now: Take Part Before time is up</h3>
        <div class="auto-grid-front-page-shows">
            <% fourRecentTakePart.forEach(recentTakePart => { %>
            <div class="autoGridPhotos transform-scale">
                <a href="/brainstv/show/<%= recentTakePart.slug %>">
                    <div class="noBorderResourcesOverlay"></div>
                </a>
                <!-- <div class="learningResourcesTopTitle"><%#= recentTakePart.showStatus %> </div> -->

                <img src="/images/show_images/<%= recentTakePart.showThumbnail %>" width="100%" height="200"
                    class="img-fluid" alt="<%= recentTakePart.showTitle %>">

                <div class="noBorderLearningResourcesBottomTitle"><%= recentTakePart.showTitle %> </div>
            </div>
            <% }) %>
        </div>
        <div class="text-center">
            <a href="/brainstv/takepart" class="btn btn-outline-light mt-3">All TakePart</a>
        </div>
    </div>






