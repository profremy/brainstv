<div class="jumbotron container-fluid">
  <a href="/brainstvadmins" class="btn btn-info marginTopBottomSmall"><i class="fa fa-arrow-circle-left"
      aria-hidden="true"></i><span>Back</span></a>

  <h4>New TV Schedule</h4><span class="role"><button type="button" class="btn btn-sm btn-warning" data-toggle="popover"
      data-trigger="focus" title="TV Schedule"
      data-content="Please make sure to create the schedules in order of A - Z or First to Last."><i
        class="fa fa-question-circle-o" aria-hidden="true"></i></button></span>
  <hr class="my-1 document-divider">

  <!-- <form action="/brainstvadmins" method="POST"> -->
  <form action="/brainstvadmins/tvschedule" method="POST">
    <%- include('_new_form_fields') %>
    <div class="row">
      <div class="col-md-5 mx-auto mb-2">
        <a href="/brainstvadmins" class="btn btn-danger"><i class="fa fa-times-circle"
            aria-hidden="true"></i><span>Cancel</span></a>
        <button type="submit" class="btn btn-success" id="loader"><i class="fa fa-cloud"
            aria-hidden="true"></i><span>Save TVSchedule</span></button>
      </div>
    </div>
  </form>

</div>