<div class="jumbotron container-fluid">
    <div class="form-group row">
        <div class="col-sm-10">
            <a href="/brainstvadmins" class="btn btn-info"><i class="fa fa-arrow-circle-left"
                    aria-hidden="true"></i><span>Back</span></a>
        </div>
    </div>
    <nav class="navbar navbar-light bg-info">
        <a class="navbar-brand">Current Site Logo</a>
    </nav>
    <hr class="my-3 document-divider">
    <% if (sitelogo != null) { %>
    <a href="/sitelogos/<%= sitelogo.id %>">
        <img width="200" height="150" src="<%= sitelogo.sitelogoPath %>" title="Change Logo or Delete"
            alt="Current site logo">
    </a>
    <% } %>
    <div class="row">
        <div class="form-group col-md-5 mb-2">
            <div class="form-group row">
                <div class="col-sm-4 my-2">

                    <a href="/sitelogos/new" class="btn btn-warning text-dark" title="New Site Logo"><i
                            class="fa fa-plus" aria-hidden="true"></i>New Site Logo</a>
                    </a>
                </div>

                <div class="col-sm-4 my-2 p-0">
                    <span class="role"><button type="button" class="btn btn-warning" data-toggle="popover"
                            data-trigger="focus" title="Site Logo"
                            data-content="You must click on the Site logo to delete it before you can use + New Site Logo button to add a new one."><i
                                class="fa fa-question-circle-o" aria-hidden="true"></i></button></span>
                </div>
            </div>
        </div>

    </div>

</div>